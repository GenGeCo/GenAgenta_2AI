# GenAgenTa 2AI - Configurazione Environment
# COPIARE QUESTO FILE IN .env E COMPILARE I VALORI

# Ambiente: development | production
ENVIRONMENT=production

# Database
DB_HOST=localhost
DB_NAME=genagenta
DB_USER=your_db_user
DB_PASS=your_db_password

# Sicurezza
JWT_SECRET=your_random_jwt_secret_min_32_chars

# AI APIs - Dual Brain Architecture
# Gemini Flash per AI veloce e economica (Google AI Studio - gratuita)
GEMINI_API_KEY=your_gemini_api_key

# OpenRouter per AI avanzata (Claude Sonnet, GPT-4, ecc)
OPENROUTER_API_KEY=your_openrouter_api_key

# Application URL
BASE_URL=https://your-domain.com

# Mapbox per le mappe
MAPBOX_TOKEN=your_mapbox_token
